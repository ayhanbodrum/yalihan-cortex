{
  "name": "yalihanemlakwarp-mcp-server",
  "version": "1.0.0",
  "description": "MCP Server for YaliHaneMlakWarp TestSprite integration",
  "type": "module",
  "main": "index.mjs",
  "scripts": {
    "start": "node index.mjs",
    "dev": "vite",
    "mcp:context7": "CONTEXT7_API_KEY=\${CONTEXT7_API_KEY} npx -y @upstash/context7-mcp --transport stdio",
    "build": "vite build",
    "test": "vitest run --environment jsdom",
    "mcp:dev": "node --watch index.mjs",
    "usta": "node scripts/usta-test.mjs",
    "usta:web-dev": "node scripts/usta-web-developer.mjs",
    "usta:ai": "node scripts/usta-ai-kesfedici.mjs",
    "usta:full": "npm run usta:ai && npm run usta:web-dev",
    "usta:report": "cat usta-web-developer-raporu.md | head -100",
    "setup": "npm install && npx husky install && chmod +x .husky/* && php artisan standard:check --type=context7 && echo '✅ Setup tamamlandı! Artık git commit otomatik kontrollü.'",
    "fix": "npm run lint:fix && npm run format && php artisan standard:check --fix",
    "lint": "npm run lint:js && npm run lint:php",
    "lint:js": "eslint resources/js --ext .js --max-warnings 0",
    "lint:php": "./vendor/bin/php-cs-fixer fix --dry-run --diff",
    "lint:fix": "eslint resources/js --ext .js --fix && ./vendor/bin/php-cs-fixer fix",
    "format": "prettier --write \"resources/**/*.{js,css,blade.php,vue}\"",
    "check": "npm run lint && php artisan standard:check",
    "scan:jquery": "node scripts/scan-jquery.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@alpinejs/collapse": "^3.15.0",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "alpinejs": "^3.15.0",
    "body-parser": "^2.2.0",
    "esbuild": "^0.25.10",
    "express": "^5.1.0",
    "laravel-vite-plugin": "^2.0.1",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "rollup": "^4.52.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "mcp",
    "testsprite",
    "yalihanemlakwarp",
    "testing"
  ],
  "author": "YaliHaneMlakWarp Team",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@testing-library/dom": "^9.3.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.38.0",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "puppeteer": "^24.24.0",
    "tailwindcss": "^3.4.18",
    "terser": "^5.44.0",
    "vitest": "^1.6.0"
  },
  "lint-staged": {
    "resources/**/*.js": [
      "prettier --write"
    ],
    "resources/**/*.css": [
      "prettier --write"
    ]
  }
}
